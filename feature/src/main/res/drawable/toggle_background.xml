<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_checked="false">
    <layer-list>
      <item android:drawable="@drawable/round_button"/>
      <item android:drawable="@drawable/ic_home"
          android:top="@dimen/default_padding"
          android:right="@dimen/default_padding"
          android:bottom="@dimen/default_padding"
          android:left="@dimen/default_padding" />
    </layer-list>
  </item>

  <item android:state_checked="true">
    <layer-list>
      <item android:drawable="@drawable/round_button"/>
      <item android:drawable="@drawable/ic_home_accent"
          android:top="@dimen/default_padding"
          android:right="@dimen/default_padding"
          android:bottom="@dimen/default_padding"
          android:left="@dimen/default_padding" />
    </layer-list>
  </item>
</selector>
